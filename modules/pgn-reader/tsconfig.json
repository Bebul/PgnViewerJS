{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "ES6",
    "outDir": "lib",
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "module": "CommonJS",
    "allowJs": true,
//    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "allowSyntheticDefaultImports": true,
    "suppressImplicitAnyIndexErrors": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "moduleResolution": "Node"
  },
  "files": ["./src/fetch.ts", "./src/index.ts", "./src/nag.ts", "./src/pgn.ts", "./src/pgn.ts", "./src/types.ts"]
}