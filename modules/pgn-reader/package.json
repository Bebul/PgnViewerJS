{
  "name": "@mliebelt/pgn-reader",
  "description": "A pgn reader (backend only) to read, understand and play chess games. Used mainly by pgn-viewer.",
  "version": "1.2.11",
  "main": "lib/index",
  "files": [
    "README.md",
    "LICENSE.md"
  ],
  "module": "modules/pgn-reaeder/src/pgn.js",
  "author": "mliebelt",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/mliebelt/PgnViewerJS.git"
  },
  "scripts": {
    "clean": "rm -fr lib/*",
    "build": "npm run clean && npm run mjs && npm run cjs && npm run umd",
    "buildw": "tsc -w",
    "mjs": "tsc -m es6 --outDir lib-esm",
    "cjs": "tsc",
    "umd": "rollup lib-esm/pgn.js -c rollup.config.js -o lib/index.umd.js -f umd",
    "test": "mocha --require ts-node/register ./test/*.ts"
  },
  "devDependencies": {
    "@tsconfig/node12": "^1.0.9",
    "@types/chess.js": "^0.11.2",
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.8",
    "mocha": "^9.1.3",
    "rollup": "^2.59.0",
    "@rollup/plugin-commonjs": "21.0.1",
    "@rollup/plugin-node-resolve": "13.1.3",
    "should": "^13.2.3",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@mliebelt/pgn-parser": "^1.4.4",
    "chess.js": "^0.12.0",
    "sync-fetch": "^0.3.1"
  }
}
